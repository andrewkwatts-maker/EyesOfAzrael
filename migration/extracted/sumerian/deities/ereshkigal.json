  "mythology_stories": {},
  "relationships": {
    "family": {
      "parents": "<li> <a class=\"corpus-link\" data-term=\"Nanna\" data-tradition=\"babylonian\" href=\"../../../mythos/babylonian/corpus-search.html?term=sin\" title=\"Search corpus for 'Nanna'\">Nanna</a> (moon god) and Ningal, in some <a class=\"corpus-link\" data-term=\"Traditions\" data-tradition=\"sumerian\" href=\"../../../mythos/sumerian/corpus-search.html?term=traditions\" title=\"View traditions in ancient texts\">traditions</a>; <a class=\"corpus-link\" data-term=\"An\" data-tradition=\"babylonian\" href=\"../../../mythos/babylonian/corpus-search.html?term=anu\" title=\"Search corpus for 'An'\">An</a> and <a class=\"corpus-link\" data-term=\"Ki\" data-tradition=\"sumerian\" href=\"../../../mythos/sumerian/corpus-search.html?term=ninhursag\" title=\"Search corpus for 'Ki'\">Ki</a> in others</li>",
      "consort": "<li> <a class=\"corpus-link\" data-term=\"Nergal\" data-tradition=\"babylonian\" href=\"../../../mythos/babylonian/corpus-search.html?term=nergal\" title=\"Search corpus for 'Nergal'\">Nergal</a> (god of war and plague, king of underworld)</li>",
      "children": "<li> Ninazu (underworld deity), Nungal (prison goddess), in some <a class=\"corpus-link\" data-term=\"Texts\" data-tradition=\"sumerian\" href=\"../../../mythos/sumerian/corpus-search.html?term=texts\" title=\"View texts in ancient texts\">texts</a></li>",
      "siblings": "<li> <a class=\"corpus-link\" data-term=\"Inanna\" data-tradition=\"babylonian\" href=\"../../../mythos/babylonian/corpus-search.html?term=ishtar\" title=\"Search corpus for 'Inanna'\">Inanna</a> (sister, whom she killed), <a class=\"corpus-link\" data-term=\"Utu\" data-tradition=\"babylonian\" href=\"../../../mythos/babylonian/corpus-search.html?term=shamash\" title=\"Search corpus for 'Utu'\">Utu</a> (sun god), in most <a class=\"corpus-link\" data-term=\"Traditions\" data-tradition=\"sumerian\" href=\"../../../mythos/sumerian/corpus-search.html?term=traditions\" title=\"View traditions in ancient texts\">traditions</a></li>"
    }
