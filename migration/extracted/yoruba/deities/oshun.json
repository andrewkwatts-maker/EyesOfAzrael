  "mythology_stories": {},
  "relationships": {
    "family": {
      "yemoja": "<li> Often described as her sister or mother. Together they represent salt water and fresh water, ocean and river.</li>",
      "oba": "<li> Her rival for Shango's love. Their conflict represents jealousy and its tragic consequences.</li>",
      "nana buruku": "<li> Sometimes described as her grandmother. Ancient swamp goddess.</li>"
    }
  },
