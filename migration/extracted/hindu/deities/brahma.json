  "relationships": {
    "family": {
      "parents": "<li> Self-born from <a class=\"corpus-link\" data-term=\"Hiranyagarbha\" data-tradition=\"hindu\" href=\"../../../mythos/hindu/corpus-search.html?term=brahma\" title=\"Search corpus for 'Hiranyagarbha'\">Hiranyagarbha</a> or born from <a class=\"inline-search-link\" href=\"./vishnu.html\">Vishnu's</a> navel lotus</li>",
      "consort": "<li> <a class=\"corpus-link\" data-term=\"Saraswati\" data-tradition=\"hindu\" href=\"../../../mythos/hindu/corpus-search.html?term=saraswati\" title=\"Search corpus for 'Saraswati'\">Saraswati</a> (Goddess of Knowledge, Arts, and Wisdom)</li>",
      "children": "<li> The Prajapatis (lords of creatures), Manu (first man), Four Kumaras (mind-born sons), Daksha, Narada</li>",
      "siblings": "<li> None (self-created), though part of <a class=\"inline-search-link\" href=\"../cosmology/creation.html\">Trimurti</a> with <a class=\"corpus-link\" data-term=\"Vishnu\" data-tradition=\"hindu\" href=\"../../../mythos/hindu/corpus-search.html?term=vishnu\" title=\"View vishnu in ancient texts\">Vishnu</a> and <a class=\"inline-search-link\" href=\"./shiva.html\">Shiva</a></li>"
    },
    "allies_enemies": {
      "allies": "<li> <a class=\"inline-search-link\" href=\"./vishnu.html\">Vishnu</a> (Preserver), <a class=\"corpus-link\" data-term=\"Saraswati\" data-tradition=\"hindu\" href=\"../../../mythos/hindu/corpus-search.html?term=saraswati\" title=\"Search corpus for 'Saraswati'\">Saraswati</a> (his consort and knowledge itself), The Devas (gods he created)</li>",
      "complex relations": "<li> <a class=\"inline-search-link\" href=\"./shiva.html\">Shiva</a> (who both protects and has cursed him), The Asuras (demons who challenge his creation)</li>"
    }
  },
