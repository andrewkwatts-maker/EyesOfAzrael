  "keyMyths": [
    {
      "title": "Nabu and the Tablets of Destiny",
      "source": "Neo-Babylonian Texts",
      "summary": "As scribe of the divine assembly, Nabu records the decrees of fate upon the Tablets of Destiny. During the Akitu festival, he accompanies Marduk and assists in determining the fates for the coming year. His stylus makes fate manifest.",
      "significance": "Establishes Nabu's cosmic role in determining and recording destiny"
    },
    {
      "title": "The Invention of Writing",
      "source": "Mesopotamian Tradition",
      "summary": "Nabu is credited with giving humanity the gift of cuneiform writing. Through the wedge-shaped stylus, he enabled mortals to record laws, preserve wisdom, and communicate across generations. Scribes were considered his 'sons.'",
      "significance": "Explains the divine origin of literacy and its sacred nature"
    },
    {
      "title": "Nabu in the Akitu Festival",
      "source": "Babylonian Ritual Texts",
      "summary": "During the New Year festival, Nabu traveled from his temple in Borsippa to Babylon, joining his father Marduk in the procession. This reunion of father and son symbolized cosmic renewal and the proper ordering of the universe.",
      "significance": "Shows Nabu's role in maintaining cosmic and social order alongside Marduk"
    }
  ],
