    "fire": {
      "title": "Fire",
      "description": "Passion, destruction, purification, and transformation. Fire consumes and renews, destroys and illuminates. The sacred flame of sacrifice, the forge of creation, the fury of divine wrath.",
      "points": []
    },
    "earth": {
      "title": "Earth",
      "description": "Stability, fertility, grounding, and endurance. The solid foundation of existence, the nurturing womb of life, the patient strength of stone and soil. Mother of all that grows.",
      "points": []
    },
    "water": {
      "title": "Water",
      "description": "Flow, emotion, life, and purification. The primordial ocean from which all emerged, the cleansing rain, the tears of gods. Adaptable, formless, essential to all existence.",
      "points": []
    },
    "air": {
      "title": "Air",
      "description": "Intellect, freedom, communication, and breath. The invisible force that carries words and spirits, the wind of change, the breath of life itself. Mind over matter.",
      "points": []
    },
    "chaos_void": {
      "title": "Chaos / Void",
      "description": "The primordial formlessness before creation, the endless potential of the abyss. Not absence but infinite possibility. The dark womb from which order emerges and to which it returns.",
      "points": []
    },
    "divine_light": {
      "title": "Divine Light",
      "description": "Order, consciousness, creation, and revelation. The first light breaking the darkness, the illuminating power of awareness, the golden radiance of divine presence and cosmic law.",
      "points": []
    },
